<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>
<h1>product情報一覧</h1>
<table>
  <thead>
    <tr>
      <th>id</th>
      <th>名前</th>
      <th>価格</th>
      <th>詳細</th>
    </tr>
  </thead>
  <tbody>
    <% @products.each do |product| %>
      <tr>
        <td><%= product.id %></td>
        <td><%= product.name %></td>
        <td><%= product.price %></td>
        <td><%= link_to "詳細", product %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<p><%= link_to "product新規登録", :new_product %></p>